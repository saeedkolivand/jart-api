generator client { provider = "prisma-client-js" }
datasource db { provider = "postgresql" url = env("DATABASE_URL") }


model User {
id String @id @default(cuid())
email String @unique
passwordHash String?
name String?
createdAt DateTime @default(now())
}


model Application {
id String @id @default(cuid())
title String
company String
source String?
status String @default("APPLIED")
createdAt DateTime @default(now())
}
